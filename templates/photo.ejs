<% layout('layout/page');
block('barPhotos', 'active');
block("title" , "Фотогалерея") %>

<div class="row main">
    <div class="left col-md-9">
        <%for(var i = photos.length - 1; photos[i] != undefined; i--) {%>
        <div class="row photoRow">
            <div class="col-md-7 albums">
                <a href="/photos/<%- photos[i].id%>">
                    <%- photos[i].photo %>
                </a>
            </div>
        </div>
        <%}%>
        <div class="col-md-7 addPhoto">
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        </div>
    </div>



</div>
<% include layout/login %>
</div>